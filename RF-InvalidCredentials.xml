<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<RaiseFault async="false" continueOnError="false" enabled="true" name="RF-InvalidCredentials">
    <DisplayName>RF-InvalidCredentials</DisplayName>
    <Properties/>
    <FaultResponse>
        <Set>
            <Headers/>
            <Payload contentType="application/json">
                { "message": "Invalid Credentials.", "error_code": "E00401", "error_description": "Please verify your credentials."}
            </Payload>
            <StatusCode>401</StatusCode>
            <ReasonPhrase>Unauthorized Access</ReasonPhrase>
        </Set>
    </FaultResponse>
    <IgnoreUnresolvedVariables>true</IgnoreUnresolvedVariables>
</RaiseFault>